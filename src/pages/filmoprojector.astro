---
import { getCollection, getEntry, render } from 'astro:content';
import ProjectPageLayout from '../layouts/ProjectPageLayout.astro';
import TableOfContents from '../components/TableOfContents.astro';
import type { CollectionKey } from 'astro:content';

interface Props {
    projectName: CollectionKey;
    projectTitle: string;
	featuredImageAltText: string;
}

const projectName = 'filmo'
const projectTitle = "Bell & Howell Filmo S 16mm Projector Rebuild"
const featuredImageAltText = "Photo of various unassembled Filmo Projector Parts"
const collectionName = await getCollection('filmo', ({ data }) => {
  return data.draft == true;
});

---

<head>
	<title>Bebop Workshop - Filmo Projector</title>
	<meta charset="UTF-8">
	<meta name="description" content="Bebop Workshop - Filmo Projector">
	<meta name="author" content="Henry Boyd">
	<link rel="canonical" href="https://www.bebopworkshop.com/filmoprojector/">
	<meta name="robots" content="index, follow">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="icon" href="/favicon.svg" type="image/x-icon">
<head/>

<ProjectPageLayout projectName={projectName} projectTitle ={projectTitle} featuredImageAltText={featuredImageAltText}>
</ProjectPageLayout>