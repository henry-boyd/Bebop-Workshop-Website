<style define:vars={{ positionXstr, positionYstr }}>
    
    :root {
        --animation-distance: 4px;
    }

    a {
        color: black;
        text-decoration: none;
    }
    
    .project-area {
        width: 100%;
        height: 100%;
        z-index: 1;
    }

    .image-box {
        margin-left: var(--positionXstr);
        margin-top: var(--positionYstr);
        display: grid;
        width: fit-content;
        justify-content: center;
        z-index: 2;
    }

    .zenith-remote-svg {
        padding: 20px 40px;
        z-index: 2;
        color: black;
    }

    .image-title {
        text-align: center;
        margin: 0;
        opacity: 0;
        transition: 1s;
    }

    .image-box:hover {
        .image-title {
            opacity: 1;
        }
    }

    @media(max-width: 1200px) {
        .image-title {
            opacity: 1;
        }
    }

    @media (max-width: 750px) {
        .project-area {
            display: grid;
            justify-content: center;
            padding: 2rem 0rem;
        }
        .image-box {
            margin: 0;
        }
    }
</style>