---
import ProjectJournalHeader from "../components/ProjectJournalHeader.astro";
import TableOfContentsDropdown from '../components/TableOfContentsDropdown.astro';
import ProjectJournalBody from "../components/ProjectJournalBody.astro";
import type { CollectionKey } from 'astro:content';

interface Props {
    projectName: CollectionKey;
    projectTitle: string;
}

const { projectName, projectTitle } = Astro.props;
---

<ProjectJournalHeader></ProjectJournalHeader>
<TableOfContentsDropdown projectName={projectName}/>
<ProjectJournalBody projectName={projectName} projectTitle ={projectTitle}></ProjectJournalBody>